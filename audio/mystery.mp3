  </span><span><span class="hljs-comment">&lt;!-- Fondo de la imagen --&gt;</span></span><span>
  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"gift-bg"</span></span><span> </span><span><span class="hljs-attr">class</span></span><span>=</span><span><span class="hljs-string">"gift-background"</span></span><span>&gt;</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;

  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">script</span></span></span><span>&gt;<span class="language-javascript">
    </span></span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-variable constant_">CONFIG</span></span><span> = {
      </span><span><span class="hljs-attr">start</span></span><span>: </span><span><span class="hljs-string">"intro"</span></span><span>,
      </span><span><span class="hljs-attr">intro</span></span><span>: {
        </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"Bienvenida a tu aventura de cumpleaños 💖. ¿Lista para empezar?"</span></span><span>,
        </span><span><span class="hljs-attr">options</span></span><span>: [
          { </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"¡Sí, vamos!"</span></span><span>, </span><span><span class="hljs-attr">next</span></span><span>: </span><span><span class="hljs-string">"path1"</span></span><span> }
        ]
      },
      </span><span><span class="hljs-attr">path1</span></span><span>: {
        </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"Caminas por un sendero lleno de sorpresas... ¿Qué camino eliges?"</span></span><span>,
        </span><span><span class="hljs-attr">options</span></span><span>: [
          { </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"El misterioso 🌌"</span></span><span>, </span><span><span class="hljs-attr">next</span></span><span>: </span><span><span class="hljs-string">"path2"</span></span><span> },
          { </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"El brillante 🌟"</span></span><span>, </span><span><span class="hljs-attr">next</span></span><span>: </span><span><span class="hljs-string">"path3"</span></span><span> }
        ]
      },
      </span><span><span class="hljs-attr">path2</span></span><span>: {
        </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"Te encuentras con una pista escondida. ¿Quieres seguirla?"</span></span><span>,
        </span><span><span class="hljs-attr">options</span></span><span>: [
          { </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"Sí, me intriga 👀"</span></span><span>, </span><span><span class="hljs-attr">next</span></span><span>: </span><span><span class="hljs-string">"gift"</span></span><span> },
          { </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"No, prefiero volver ↩️"</span></span><span>, </span><span><span class="hljs-attr">next</span></span><span>: </span><span><span class="hljs-string">"path1"</span></span><span> }
        ]
      },
      </span><span><span class="hljs-attr">path3</span></span><span>: {
        </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"El camino brillante te lleva directo a la sorpresa final."</span></span><span>,
        </span><span><span class="hljs-attr">options</span></span><span>: [
          { </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"¡Descubrir el regalo! 🎁"</span></span><span>, </span><span><span class="hljs-attr">next</span></span><span>: </span><span><span class="hljs-string">"gift"</span></span><span> }
        ]
      },
      </span><span><span class="hljs-attr">gift</span></span><span>: {
        </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">"Tu regalo es un viaje a un lugar mágico... ❤️"</span></span><span>,
        </span><span><span class="hljs-attr">options</span></span><span>: []
      }
    };

    </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">showNode</span></span><span>(</span><span><span class="hljs-params">nodeKey</span></span><span>) {
      </span><span><span class="hljs-keyword">const</span></span><span> node = </span><span><span class="hljs-variable constant_">CONFIG</span></span><span>[nodeKey];
      </span><span><span class="hljs-keyword">const</span></span><span> storyDiv = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"story"</span></span><span>);
      </span><span><span class="hljs-keyword">const</span></span><span> optionsDiv = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"options"</span></span><span>);

      storyDiv.</span><span><span class="hljs-property">textContent</span></span><span> = node.</span><span><span class="hljs-property">text</span></span><span>;
      optionsDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;

      node.</span><span><span class="hljs-property">options</span></span><span>.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">opt</span></span></span><span> =&gt; {
        </span><span><span class="hljs-keyword">const</span></span><span> btn = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"button"</span></span><span>);
        btn.</span><span><span class="hljs-property">textContent</span></span><span> = opt.</span><span><span class="hljs-property">text</span></span><span>;
        btn.</span><span><span class="hljs-property">onclick</span></span><span> = </span><span><span class="hljs-function">() =&gt;</span></span><span> </span><span><span class="hljs-title function_">showNode</span></span><span>(opt.</span><span><span class="hljs-property">next</span></span><span>);
        optionsDiv.</span><span><span class="hljs-title function_">appendChild</span></span><span>(btn);
      });

      </span><span><span class="hljs-comment">// Cuando llegamos al regalo, mostramos la imagen de fondo</span></span><span>
      </span><span><span class="hljs-keyword">if</span></span><span> (nodeKey === </span><span><span class="hljs-string">"gift"</span></span><span>) {
        </span><span><span class="hljs-keyword">const</span></span><span> bg = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"gift-bg"</span></span><span>);
        bg.</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">backgroundImage</span></span><span> = </span><span><span class="hljs-string">"url('https://images.unsplash.com/photo-1613932534449-3d1cc403a53a?ixlib=rb-4.0.3&amp;auto=format&amp;fit=crop&amp;w=1920&amp;q=80')"</span></span><span>;
        </span><span><span class="hljs-built_in">setTimeout</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> bg.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">"reveal"</span></span><span>), </span><span><span class="hljs-number">100</span></span><span>);
      }
    }

    </span><span><span class="hljs-comment">// Iniciar historia</span></span><span>
    </span><span><span class="hljs-title function_">showNode</span></span><span>(</span><span><span class="hljs-variable constant_">CONFIG</span></span><span>.</span><span><span class="hljs-property">start</span></span><span>);
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">script</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">body</span></span></span><span>&gt;
